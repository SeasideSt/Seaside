{ #category : #accessing }
WAWelcomeFiles >> selectorsToInclude [
	^ #(welcomeCss syntaxHighlightingCss)
]

{ #category : #resources }
WAWelcomeFiles >> syntaxHighlightingCss [	^ '
span.patternUnary  {
color:#000000;
font-weight:bold;
}

span.patternKeyword  {
color:#000000;
font-weight:bold;
}

