Class {
	#name : #WATestingFiles,
	#superclass : #WAFileLibrary,
	#category : 'Seaside-Tests-Core-Libraries'
}

{ #category : #private }
WATestingFiles >> defaultFileHandler [
	"Superclass could have implementation of (WAFileHandler default), 
	but that is not defined at time test runs when they are loaded in order."

	^ WADispatcher new register: WAFileHandler new at: 'files'
]

{ #category : #private }
WATestingFiles >> nonFileSelectors [
	^ super nonFileSelectors copyWith: #defaultFileHandler
]

