{ #category : #rendering }
JQIconsFunctionalTest >> renderContentOn: html [
	html unorderedList
		class: 'icons';
		class: 'ui-widget';
		class: 'ui-helper-clearfix';
		script: (html jQuery new
			find: 'li';
			hover: (html jQuery new 
				addClass: 'ui-state-hover')
			out: (html jQuery new
				removeClass: 'ui-state-hover'));
		with: [ self renderIconsOn: html ]
]

{ #category : #rendering }
JQIconsFunctionalTest >> renderIconsOn: html [
	self iconClasses do: [ :each |
		html listItem
			title: each;
			class: 'ui-corner-all';
			class: 'ui-state-default';
			with: [
				html span
					class: 'ui-icon';
					class: each ] ]
]

{ #category : #accessing }
JQIconsFunctionalTest >> selectors [
	^ #(renderContentOn: renderIconsOn:)
]

{ #category : #hooks }
JQIconsFunctionalTest >> style [
	^ 'ul.icons, ul.icons li { margin: 0; padding: 0; list-style-type: none; }
ul.icons li { float: left; margin: 2px; padding: 2px; }'
]

{ #category : #accessing }
JQIconsFunctionalTest >> title [
	^ 'Icon Theming'
]

{ #category : #accessing }
JQIconsFunctionalTest >> url [
	^ 'http://docs.jquery.com/UI/Theming/API'
]
