"
I return a static 16k byte array. This number is the number of bytes in the Seaside homepage. I'm useful for benchmarking the HTTP server and server adapter.
"
Class {
	#name : #WAFastRequestHandler,
	#superclass : #WARequestHandler,
	#instVars : [
		'pageContent'
	],
	#category : 'Seaside-Benchmark'
}

{ #category : #'class initialization' }
WAFastRequestHandler class >> register [
	WADispatcher default
		register: self new at: 'fast' 
]

{ #category : #handling }
WAFastRequestHandler >> handleFiltered: aRequestContext [
	aRequestContext respond: [ :response |
		response
			binary;
			contentType: WAMimeType textHtml;
			nextPutAll: self pageContent ]
]

